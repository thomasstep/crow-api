{
  "name": "crow-api",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "description": "AWS CDK Construct to create a serverless API based on file structure.",
  "homepage": "https://github.com/thomasstep/crow-api",
  "repository": "github:thomasstep/crow-api",
  "bugs": {
    "url": "https://github.com/thomasstep/crow-api/issues",
    "email": "thomas@thomasstep.com"
  },
  "author": {
    "name": "Thomas Step",
    "email": "thomas@thomasstep.com",
    "url": "https://thomasstep.com/"
  },
  "keywords": [
    "awscdk",
    "aws lambda",
    "serverless"
  ],
  "main": "index.js",
  "files": [
    "README.md",
    "index.ts",
    "cdk.json"
  ],
  "bin": {
    "crow-api": "bin/crow-api.ts"
  },
  "scripts": {
    "build": "jsii",
    "watch": "jsii --watch",
    "package": "jsii-pacmak",
    "release": "jsii-release",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk": "2.0.0",
    "constructs": "10.0.9",
    "jest": "^26.4.2",
    "jsii": "^1.46.0",
    "jsii-config": "^1.46.0",
    "jsii-pacmak": "^1.46.0",
    "jsii-release": "^0.2.198",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.0.0",
    "fs-extra": "^10.0.0",
    "source-map-support": "^0.5.16"
  },
  "bundledDependencies": [
    "fs-extra",
    "source-map-support"
  ],
  "peerDependencies": {
    "constructs": "^10.0.9"
  },
  "stability": "experimental",
  "types": "index.d.ts",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "full",
    "targets": {
      "python": {
        "distName": "crow-api.core",
        "module": "crow_api.core"
      }
    }
  }
}
